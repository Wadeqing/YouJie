<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <FrameLayout
        android:id="@+id/framelayout_me_cosulting_fragment"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:visibility="gone"
        android:layout_gravity="center">

        <com.youjie.android.pulltorefresh.PullToRefreshListView
            android:id="@+id/listview_me_quiz"
            android:layout_width="match_parent"
            android:layout_height="match_parent"></com.youjie.android.pulltorefresh.PullToRefreshListView>

        <LinearLayout
            android:id="@+id/lineaylayout_none"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:gravity="center"
            android:orientation="vertical">

            <ImageView
                android:id="@+id/imageview_none"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:src="@drawable/cqd_counseling_bg_default" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="45dp"
                android:text="这里什么都没有，去试试打通电话吧~"
                android:textColor="@color/text_color_99gray"
                android:textSize="@dimen/text_size_middle_s" />
        </LinearLayout>
    </FrameLayout>
</LinearLayout>